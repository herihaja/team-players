                    </li>
                </ul>
            </div>
        </aside>
        <main class="main-content position-relative max-height-vh-100 h-100 border-radius-lg">
            <div id="navbarBlur" class="shadow-none navbar navbar-main navbar-expand-lg border-radius-xl px-0 mx-4 mt-4"></div>
            <div id="appa" class="py-4 container-fluid">
                {% block body %}{% endblock %}
            </div>
        </main>
    </div>
        {# block javascripts %}
            <script src="{{ asset('build/js/app.js') }}"></script>
        {% endblock #}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
